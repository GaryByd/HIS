<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luojiawei.his_service.mapper.MedicationRecommendationsMapper">

    <select id="selectByDiagnosisId"
            resultType="com.luojiawei.his_service.domain.po.MedicationRecommendations">
        select * from medication_recommendations where diagnosis_id = #{diagnosisId}
    </select>
</mapper>
